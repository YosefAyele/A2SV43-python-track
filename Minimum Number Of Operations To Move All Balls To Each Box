class Solution:
    def minOperations(self, boxes: str) -> List[int]:
        # brute force solution
        res=[0]*len(boxes)

        for i in range(len(boxes)):
            operations=0
            for j in range(i):
                if boxes[j]=="1":
                    operations+=(abs(i-j))
            for k in range(i,len(boxes)):
                if boxes[k]=="1":
                    operations+=(abs(i-k))
            res[i]=operations
        
        return res
